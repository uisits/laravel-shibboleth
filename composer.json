{
  "name": "uisits/laravel-shibboleth",
  "description": "Enable basic Shibboleth support for Laravel 5.x",
  "authors": [
    {
      "name": "UIS ITS",
      "email": "its@uis.edu"
    }
  ],
  "require": {
    "illuminate/support": "5.*",
    "mrclay/shibalike": "1.0.0",
    "laravel/framework": "^5.4.0 || ^5.5.0 || ^5.6.0 || ^5.7.0 || ^5.8.0",
    "tymon/jwt-auth": "^1.0.0-rc.4.1"
  },
  "autoload": {
    "psr-4": {
      "StudentAffairsUwm\\Shibboleth\\": "src/StudentAffairsUwm/Shibboleth"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StudentAffairsUwm\\Shibboleth\\Tests\\Stubs\\": "tests/setup/Stubs",
      "App\\": "tests/setup/app"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "3.8.*"
  },
  "extra": {
    "laravel": {
      "providers": ["StudentAffairsUwm\\Shibboleth\\ShibbolethServiceProvider"]
    }
  }
}
